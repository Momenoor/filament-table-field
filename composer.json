{
    "name": "momenoor/filament-table-field",
    "description": "Editable table field for Filament Forms with relationship support",
    "keywords": [
        "filament",
        "laravel",
        "table-field",
        "relation-manager",
        "forms"
    ],
    "homepage": "https://github.com/momenoor/filament-table-field",
    "support": {
        "issues": "https://github.com/momenoor/filament-table-field/issues",
        "source": "https://github.com/momenoor/filament-table-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Momenoor",
            "email": "momen.noor@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "autoload": {
        "psr-4": {
            "Momenoor\\FilamentTableField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Momenoor\\FilamentTableField\\FilamentTableFieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
